{
  "headers": [
    {
      "source": "/vfs/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        },
        {
          "key": "Content-Type",
          "value": "application/json"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/((?!vfs|api).*)",
      "destination": "/index.html"
    }
  ]
}
